{
  "manifest_version": 3,
  "name": "Demo extension",
  "description": "Demo extension",
  "version": "1.0.0",
  "action": {
    "default_popup": "popup.html",
    "default_title": "Demo extension",
    "default_icon": "icon.png"
  },

  "permissions": ["tabs", "bookmarks", "storage", "identity"],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "options_page": "options.html",
  "background": {
    "service_worker": "background.js"
  }
}
